 #include<stdio.h>
int main()
{
	float principal,rate,time,si;
	printf("entre principal : ");
	scanf("%f",&principal);
  	printf("entre rate : ");
	scanf("%f",&rate);
	printf("entre time : ");
	scanf("%f",& time);
	si=(principal*rate*time)/100;
	printf("your simple interest is :%f",si);
	return 0;
}
